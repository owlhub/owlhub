{
  "name": "owlhub",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev --turbopack",
    "build": "npx prisma generate && next build",
    "start": "next start",
    "lint": "next lint",
    "make-first-user-super-user": "npx ts-node -P tsconfig.scripts.json scripts/makeFirstUserSuperUser.ts",
    "seed": "npx prisma generate && npx ts-node -P tsconfig.scripts.json prisma/seed/seed.ts",
    "migrate": "yarn migrate:deploy && npx prisma generate && yarn seed",
    "migrate:create": "npx prisma migrate dev --name",
    "migrate:deploy": "npx prisma migrate deploy",
    "migrate:reset": "npx prisma migrate reset",
    "migrate:check": "npx ts-node -P tsconfig.scripts.json scripts/check-migrations.ts"
  },
  "dependencies": {
    "react": "^19.0.0",
    "react-dom": "^19.0.0",
    "next": "15.3.4",
    "next-auth": "^5.0.0-beta.4",
    "@prisma/client": "^5.0.0",
    "@auth/prisma-adapter": "^1.0.0",
    "bcryptjs": "^2.4.3",
    "@fortawesome/fontawesome-svg-core": "^6.5.1",
    "@fortawesome/free-solid-svg-icons": "^6.5.1",
    "@fortawesome/free-regular-svg-icons": "^6.5.1",
    "@fortawesome/free-brands-svg-icons": "^6.5.1",
    "@fortawesome/react-fontawesome": "^0.2.0",
    "react-markdown": "^9.0.1",
    "remark-gfm": "^4.0.0",
    "pg": "^8.11.3",
    "@aws-sdk/client-sts": "^3.485.0",
    "@aws-sdk/client-iam": "^3.485.0"
  },
  "devDependencies": {
    "typescript": "^5",
    "@types/node": "^20",
    "@types/react": "^19",
    "@types/react-dom": "^19",
    "@types/bcryptjs": "^2.4.2",
    "@tailwindcss/postcss": "^4",
    "@tailwindcss/typography": "^0.5.10",
    "tailwindcss": "^4",
    "eslint": "^9",
    "eslint-config-next": "15.3.4",
    "@eslint/eslintrc": "^3",
    "prisma": "^5.0.0",
    "ts-node": "^10.9.1",
    "@types/pg": "^8.10.9"
  }
}
